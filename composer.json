{
    "name": "danmatthews/random-url-safe-token",
    "description": "URL Safe Base64 tokens to be used as IDs in place of UUIDs.",
    "keywords": [
        "danmatthews",
        "url-safe-base64-id"
    ],
    "homepage": "https://github.com/danmatthews/url-safe-base64-id",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dan Matthews",
            "email": "dan@danmatthews.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Danmatthews\\RandomSecureUrlSafeToken\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danmatthews\\RandomSecureUrlSafeToken\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
